void setup() {
  pinMode(6,OUTPUT); //BLUE
   pinMode(7,OUTPUT); // RED
  
}

void loop() {
  for (int i = 0; i<3; i++)
  {
    // BLUE (PIN 6)
    digitalWrite(6, HIGH);
    digitalWrite(7, LOW);
    delay(100);
    digitalWrite(6, LOW);
    delay(100);    
  }
   for (int i = 0; i<3; i++)
   {
   // GREEN (PIN 7)
    digitalWrite(7, HIGH);
    digitalWrite(6, LOW);
    delay(100);
    digitalWrite(7, LOW);
    delay(100);
   } 

}
